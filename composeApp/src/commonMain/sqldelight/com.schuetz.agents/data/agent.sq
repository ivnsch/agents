import kotlin.Boolean;

CREATE TABLE agent (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL,
  is_me INTEGER AS Boolean NOT NULL,
  avatar_url Text NOT NULL,
  provider TEXT NOT NULL,
  model TEXT,
  api_key TEXT
);

selectAll:
SELECT id, name, is_me, avatar_url, provider, model, api_key FROM agent;

selectMe:
SELECT id, name, is_me, avatar_url FROM agent WHERE is_me = 1;

insert:
INSERT INTO agent(name, is_me, avatar_url, provider, model, api_key) VALUES (?, ?, ?, ?, ?, ?);

deleteAll:
DELETE FROM agent;

countAgents:
SELECT COUNT(*) FROM agent;

lastInsertId:
SELECT last_insert_rowid();
